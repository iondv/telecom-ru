<%
let result = {
  title: item.get('name')
}

if (item.get('chislennost')) {
  result.title += ' (' + item.property('chislennost').evaluate() + ' чел.)';
}

let itemSvyaz = item.property('svyaz').evaluate();
if (itemSvyaz) {
%>


<ul class='balloon-attrlist' style='width:225px'>
  <li><%- result.title %></li>
</ul>
<ul class='mt10'>
  <%
  let coll = itemSvyaz.property('statsionar').evaluate();
  if (coll && coll.length) {
    for (let elem of coll) {
    let collOp = elem.property('operator').evaluate();

    let collAts = elem.property('stacionSvyaz').getDisplayValue();
  if (collOp) {
  %>
  <li><a class='map-modal-link' href='/registry/telecom-en@statsion/view/statsionar@telecom-en/<%- elem.id %>?readonly=on&condensed=on'>
      <%- collOp + ' (' + collAts + ')'%>
    </a>
  </li>
  <% } } } } %>
</ul>